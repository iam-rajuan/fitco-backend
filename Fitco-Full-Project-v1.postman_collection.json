        {
          "name": "Login User",
          "event": [
            {
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "const json = pm.response.json();",
                  "if (json.accessToken) pm.collectionVariables.set('userAccessToken', json.accessToken);",
