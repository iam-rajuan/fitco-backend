              "body": "{\n  \"user\": {\n    \"id\": \"65f0...\",\n    \"email\": \"john@example.com\",\n    \"role\": \"user\",\n    \"name\": \"John Doe\"\n  },\n  \"accessToken\": \"ey...\",\n  \"refreshToken\": \"ey...\"\n}"
